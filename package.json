{
  "name": "ts-bundle-template",
  "version": "1.0.0",
  "description": "add description",
  "keywords": [
    "add keywords"
  ],
  "homepage": "https://github.com/productdevbookcom/ts-bundle-template",
  "bugs": "https://github.com/productdevbookcom/ts-bundle-template/issues",
  "author": "add author",
  "repository": {
    "type": "git",
    "url": "https://github.com/productdevbookcom/ts-bundle-template.git"
  },
  "funding": "https://github.com/sponsors/productdevbook",
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.28.0",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "vite build --mode production",
    "dev": "vite",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm build && bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage"
  },
  "license": "MIT",
  "peerDependencies": {
    "vue": "=>3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/test-utils": "^2.3.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.35.0",
    "jsdom": "^21.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^0.29.2",
    "vue": "^3.2.47"
  },
  "publishConfig": {
    "access": "public"
  }
}